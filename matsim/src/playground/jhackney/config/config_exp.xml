<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd"
	[
		<!ENTITY PROJBASE     ".">
		<!ENTITY STUDYBASE    "C:/Documents and Settings/jhackney/My Documents/sandbox00/vsp-cvs/studies/jhackney/input">
		<!ENTITY RUNSBASE     "runs">
		<!ENTITY OUTPUTBASE   "C:/Documents and Settings/jhackney/My Documents/sandbox00/vsp-cvs/studies/jhackney/output">
		<!ENTITY INPUTBASE    "input">
		<!ENTITY ANALYSISBASE "analysis">
		<!ENTITY LOCALDTDBASE "dtd">
		<!ENTITY HOMEPAGEBASE "http://www.matsim.org">
	]
>
<config xml:lang="de-CH">

<!-- ====================================================================== -->
<!-- output validation for world_v2, network_v1, facilities_v1 and plans_v4 -->
<!-- use org.matsim.ValidationTest as the main class                        -->
<!-- ====================================================================== -->

	<module name="config">
		<param name="outputConfigFile" value="&OUTPUTBASE;/output_config.xml" />
	</module>

<!-- ====================================================================== -->

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="outputTimeFormat" value="HH:mm:ss" />
		<param name="useRoadPricing" value="no" />
		<param name="localDTDBase" value="&LOCALDTDBASE;/" />
	</module>

<!-- ====================================================================== -->

	<module name="world">
		<param name="inputWorldFile" value="&STUDYBASE;/world.xml" />
		<param name="outputWorldFile" value="&OUTPUTBASE;/output_world.xml" />
	</module>

<!-- ====================================================================== -->

	<module name="network">
		<param name="inputNetworkFile" value="&STUDYBASE;/network.xml" />
		<param name="outputNetworkFile" value="&OUTPUTBASE;/output_network.xml" />
	</module> <!-- network -->

<!-- ====================================================================== -->

	<module name="facilities">
		<param name="inputFacilitiesFile" value="&STUDYBASE;/facilities.xml" />
		<param name="outputFacilitiesFile" value="&OUTPUTBASE;/output_facilities.xml" />
	</module>

<!-- ====================================================================== -->

	<module name="plans">
		<param name="inputPlansFile" value="&STUDYBASE;/plans.xml" />
		<param name="outputPlansFile" value="&OUTPUTBASE;/output_plans.xml" />
		<param name="outputVersion" value="v4" />
		<param name="outputSample" value="1.00" />
		<param name="switchOffPlansStreaming" value="yes" />
	</module> <!-- plans -->
	
<!-- ====================================================================== -->

	<module name="simulation">
		<!-- "start/endTime" of MobSim (00:00:00 == take earliest activity time/ run as long as active vehicles exist) -->
		<param name="startTime" value="00:00:00" />
		<param name="endTime" value="00:00:00" />
	 
		<!-- capdivider gets multiplied with this valus: If you use only a percentage of vehicles flow cap must be modified by this to ge reasonable behaviour -->
		<param name = "flowCapacityFactor"	value = "1.0"/>
		<param name = "storageCapacityFactor"	value = "1.0"/>

		<!-- "stuckTime" after how many seconds in buffer shall we call a vehicle stuck -->
		<param name="stuckTime" value="100" />
		<param name="removeStuckVehicles" value="no" />
	</module> <!-- simulation -->

<!-- ====================================================================== -->
	<module name="strategy">
		<!-- how many plans agents maintain in memory; during an iteration where the agent creates a new plan -->
		<!-- it may contain 1 more than this number of plans; but the extra will be deleted at the next iter. (worst plan is deleted) -->
		<!-- size of 0 means unlimited (except by memory!) -->
		<param name="maxAgentPlanMemorySize" value="1" />
		
		<!-- some external exes (i.e. TimeAllocationMutator) need their own old c++ config file -->
		<param name="ExternalExeConfigTemplate" value="config_template_external_exe.xml" />
		<!-- this is were the temporary plans files for external exe get written -->
		<param name="ExternalExeTmpFileRootDir" value="&OUTPUTBASE;/" />
		<!-- how many secondss will pass until an external exe run is aborted (1h=default)-->
		<param name="ExternalExeTimeOut" value="3600" />
		
		<!-- Add Modules here, Probabillity values get all summed up and normalized to 1.0 -->
	</module> <!-- strategy -->	
	
<!-- ====================================================================== -->
	<module name="planCalcScore">
		<!-- learningRate (lR): used for score-averaging: score = lR*newScore + (1-lR)*oldScore -->
		<param name="learningRate" value="1.0" />
	
		<param name="BrainExpBeta" value="2.0" />
		<param name="lateArrival" value="-18" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+6" />
		<param name="traveling" value="-6" />
		<param name="waiting" value="-0" />
		
		<!-- Activity types and their parameters 
		 The only required parameters are:
	
		      * activityType_# (must be unique as well)
		      * activityPriority_#
		      * activityTypicalDuration_#
	
		     The rest are optional.
		     
		?? superceded by Factivities ?? -->	
		
		<param name="activityType_0"            value="home" /> <!-- home -->
		<param name="activityPriority_0"        value="1" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityMinimalDuration_0" value="08:00:00" />	
		
		<param name="activityType_1"            value="work" /> <!-- work -->
		<param name="activityPriority_1"        value="1" />
		<param name="activityTypicalDuration_1" value="08:00:00" />
		<param name="activityMinimalDuration_1" value="06:00:00" />
		<param name="activityOpeningTime_1"     value="07:00:00" />
		<param name="activityLatestStartTime_1" value="09:00:00" />
		<param name="activityEarliestEndTime_1" value="" />
		<param name="activityClosingTime_1"     value="18:00:00" />
			
		<param name="activityType_2"            value="education" /> <!-- edu -->
		<param name="activityPriority_2"        value="1" />
		<param name="activityTypicalDuration_2" value="06:00:00" />
		<param name="activityMinimalDuration_4" value="04:00:00" />
		<param name="activityOpeningTime_4"     value="07:00:00" />
		<param name="activityLatestStartTime_4" value="09:00:00" />
		<param name="activityEarliestEndTime_4" value="" />
		<param name="activityClosingTime_4"     value="18:00:00" />
			
		<param name="activityType_3"            value="shop" /> <!-- shop -->
		<param name="activityPriority_3"        value="1" />
		<param name="activityTypicalDuration_3" value="02:00:00" />
		<param name="activityMinimalDuration_3" value="01:00:00" />
		<param name="activityOpeningTime_3"     value="08:00:00" />
		<param name="activityLatestStartTime_3" value="" />
		<param name="activityEarliestEndTime_3" value="" />
		<param name="activityClosingTime_3"     value="20:00:00" />
	
		<param name="activityType_4"            value="leisure" /> <!-- leisure -->
		<param name="activityPriority_4"        value="1" />
		<param name="activityTypicalDuration_4" value="02:00:00" />
		<param name="activityMinimalDuration_4" value="01:00:00" />
		<param name="activityOpeningTime_4"     value="06:00:00" />
		<param name="activityLatestStartTime_4" value="" />
		<param name="activityEarliestEndTime_4" value="" />
		<param name="activityClosingTime_4"     value="24:00:00" />
	</module>
<!-- ====================================================================== -->

	<module name="controler">
		<param name="outputDirectory" value="&OUTPUTBASE;" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="3" />
	</module>
<!-- ====================================================================== -->

	<module name="socialnetwork" >
		<param name="degree_saturation_rate" value="0" />
		<param name="edge_type" value="UNDIRECTED" />
		<param name="factype_ns" value="home,leisure,shop,education,work" />
		<param name="fract_introduce_friends" value=".05" />
		<param name="fract_ns_interact" value=".05" />
		<param name="fract_s_interact" value=".05" />
		<param name="kbar" value="0" />
		<param name="max_sn_iter" value="10" />
		<param name="nonspatial_interactor_type" value="random" />
		<param name="num_ns_interactions" value="1" />
		<param name="outputDirSocialNets" value="C:/Documents and Settings/jhackney/My Documents/sandbox00/vsp-cvs/studies/jhackney/output/socialnets/" />
		<param name="prob_befriend" value="1." />
		<param name="replanning_interval" value="1" />
		<param name="s_weights" value="1.,1.,1.,1.,1." />
		<param name="socnetalgorithm" value="random" />
		<param name="socnetlinkremovalage" value="0" />
		<param name="socnetlinkremovalalgorithm" value="random_node_degree" />
		<param name="socnetlinkremovalp" value="0.005" />
		<param name="socnetlinkstrengthalgorithm" value="constant" />
		<param name="spatial_interactor_type" value="random" />
		<param name="switch_weights" value="0.0,0.0,1.0,0.0,1.0" />
	</module>



<!-- ====================================================================== -->
<!-- ====================================================================== -->

</config>