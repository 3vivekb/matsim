<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.vsp.tu-berlin.de/projects/Matsim/data/dtd/config_v1.dtd"
	[
		<!ENTITY CVSROOT      "C:/Documents and Settings/anhorni/sandbox/vsp-cvs">
		<!ENTITY PROJBASE     "devel/matsim/matsimJ">
		<!ENTITY STUDYBASE    "studies/triangle">
		<!ENTITY OUTPUTBASE   "triangle_out">
		<!ENTITY HOMEPAGEBASE "http://www.vsp.tu-berlin.de/projects/Matsim/">
		<!ENTITY LOCALDTDBASE "devel/matsim/matsimJ/dtd">
		<!ENTITY LOCALXSDBASE "devel/matsim/matsimJ/dtd">
	]
>
<config xml:lang="de-CH">

<!-- ====================================================================== -->
<!-- triangle test                                                          -->
<!-- use org.matsim.TriangleTest as the main class                        -->
<!-- ====================================================================== -->

	<module name="config" >
		<param name="outputConfigDTD" value="&CVSROOT;/&PROJBASE;/input/dtd/config_v1.dtd" />
		<param name="outputConfigFile" value="&CVSROOT;/&PROJBASE;/&OUTPUTBASE;/output_config.xml" />
		<param name="outputVersion" value="v1" />
	</module>

<!-- ====================================================================== -->

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="outputTimeFormat" value="HH:mm:ss" />
	</module> <!-- global -->

<!-- ====================================================================== -->

	<module name="world">
		<param name="inputWorldFile" value="&CVSROOT;/&STUDYBASE;/world/world_cell20x20_blk_muni.xml" />
		<param name="localInputDTD" value="&CVSROOT;/&LOCALDTDBASE;/world_v2.dtd" />
		<param name="outputWorldFile" value="&CVSROOT;/&STUDYBASE;/&OUTPUTBASE;/output_world.xml" />
		<param name="outputWorldDTD" value="&HOMEPAGEBASE;/data/dtd/world_v2.dtd" />
		<param name="outputVersion" value="v2" />
	</module> <!-- world -->

<!-- ====================================================================== -->

	<module name="network">
		<param name="inputNetworkFile" value="&CVSROOT;/&STUDYBASE;/network/net.xml" />
		<param name="localInputDTD" value="&CVSROOT;/&LOCALDTDBASE;/network_v1.dtd" />
		<param name="outputNetworkFile" value="&CVSROOT;/&STUDYBASE;/&OUTPUTBASE;/output_network.xml" />
		<param name="outputNetworkDTD" value="&HOMEPAGEBASE;/data/dtd/network_v1.dtd" />
		<param name="outputVersion" value="v1" />
	</module> <!-- network -->
	
	<!-- ====================================================================== -->

	<module name="counts">
		<param name="inputCountsFile" value="&CVSROOT;/&STUDYBASE;/test-counts/counts.xml" />
		<param name="localInputXSD" value="&CVSROOT;/&LOCALXSDBASE;/counts_v1.xsd" />
		<param name="outputCountsFile" value="&CVSROOT;/&STUDYBASE;/&OUTPUTBASE;/output_counts.xml" />
		<param name="outputCountsXSD" value="&CVSROOT;/&LOCALXSDBASE;/counts_v1.xsd" />
		<param name="outputVersion" value="v1" />
	</module> <!-- counts -->

<!-- ====================================================================== -->

	<module name="facilities">
		<param name="inputFacilitiesFile" value="&CVSROOT;/&STUDYBASE;/test-facilities/facilities_v1.xml" />
		<param name="localInputDTD" value="&CVSROOT;/&LOCALDTDBASE;/facilities_v1.dtd" />
		<param name="outputFacilitiesFile" value="&CVSROOT;/&STUDYBASE;/&OUTPUTBASE;/output_facilities.xml" />
		<param name="outputFacilitiesDTD" value="&HOMEPAGEBASE;/data/dtd/facilities_v1.dtd" />
		<param name="outputVersion" value="v1" />
	</module> <!-- landuse -->

<!-- ====================================================================== -->

	<module name="plans">
		<param name="outputPlansFile" value="&CVSROOT;/&STUDYBASE;/&OUTPUTBASE;/output_plans.xml" />
		<param name="outputPlansDTD" value="&HOMEPAGEBASE;/data/dtd/plans_v4.dtd" />
		<param name="outputVersion" value="v4" />
		<param name="outputSample" value="1.00" />
		<param name="switchOffPlansStreaming" value="yes" />
	</module> <!-- plans -->

	<module name="plansCreateFromNetwork">
		<param name="person2CellsProportion" value="2" />
	</module> <!-- personCreateFromNetwork -->

<!-- ====================================================================== -->
<!-- ====================================================================== -->
<!-- ====================================================================== -->

</config>
